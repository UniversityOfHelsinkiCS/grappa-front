{
  "name": "grappa-front",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "lint": "eslint --ext .js,.jsx src/ test/ || true",
    "lint:fix": "eslint --ext .js,.jsx src/ test/ --fix",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.prod.js --progress --colors",
    "postinstall": "node ./is_prod.js && webpack --config webpack.config.prod.js --progress --colors || true",
    "start": "node app.js",
    "pm2:start": "pm2 start -n grappa-front npm -- start",
    "test": "ava",
    "test:old": "export NODE_ENV=testing|| set NODE_ENV=testing&& istanbul cover node_modules/mocha/bin/_mocha -- --compilers js:babel-core/register --require ./test/testhelper.js  \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch",
    "test:noc": "export NODE_ENV=testing|| set NODE_ENV=testing&& mocha --compilers js:babel-core/register --require ./test/testhelper.js  \"test/**/*@(.js|.jsx)\"",
    "instrument": "istanbul instrument . -o instrument/"
  },
  "engines": {
    "node": "8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ultra-hyper-storm-ohtuprojekti/grappa-front.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ultra-hyper-storm-ohtuprojekti/grappa-front/issues"
  },
  "homepage": "https://github.com/ultra-hyper-storm-ohtuprojekti/grappa-front#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "classnames": "^2.2.3",
    "compression": "^1.6.2",
    "dotenv": "^2.0.0",
    "express": "^4.13.4",
    "history": "^4.4.0",
    "immutable": "^3.7.6",
    "moment": "^2.12.0",
    "pm2": "^2.5.0",
    "react": "^15.6.1",
    "react-datepicker": "^0.34.0",
    "react-dom": "^15.6.1",
    "react-dropzone": "^3.5.1",
    "react-redux": "^4.0.6",
    "react-router": "^3.0.0",
    "react-validation": "^2.10.9",
    "reactable": "^0.14.1",
    "redux": "^3.5.2",
    "redux-immutablejs": "0.0.8",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^2.1.0",
    "socket.io-client": "^1.7.2",
    "validator": "^6.1.0",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "coveralls": "^2.11.9",
    "enzyme": "^2.9.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-react": "^6.7.1",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "^9.8.3",
    "mocha": "^3.1.2",
    "nyc": "^11.1.0",
    "react-test-renderer": "^15.6.1",
    "sinon": "^1.17.3",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js",
      "test/**/*.spec.jsx"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": true,
    "powerAssert": false,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
