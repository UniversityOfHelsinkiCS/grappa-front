{
  "name": "grappa-front",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "lint": "eslint --ext .js,.jsx src/ test/ || true",
    "lint:fix": "eslint --ext .js,.jsx src/ test/ --fix",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.prod.js --progress --colors",
    "postinstall": "node ./is_prod.js && webpack --config webpack.config.prod.js --progress --colors || true",
    "start": "node app.js",
    "pm2:start": "pm2 start -n grappa-front npm -- start",
    "test": "NODE_ENV=testing ava",
    "nyan": "NODE_ENV=testing ava --tap | tap-nyan",
    "coverage": "NODE_ENV=testing nyc ava",
    "report": "nyc report --reporter=html",
    "watch": "ava --watch"
  },
  "engines": {
    "node": "8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UniversityOfHelsinkiCS/grappa-frontend.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UniversityOfHelsinkiCS/grappa-frontend/issues"
  },
  "homepage": "https://github.com/UniversityOfHelsinkiCS/grappa-frontend#readme",
  "dependencies": {
    "axios": "^0.15.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.3.13",
    "classnames": "^2.2.6",
    "compression": "^1.7.2",
    "dotenv": "^2.0.0",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "moment": "^2.22.2",
    "pm2": "^2.10.4",
    "react": "^15.6.2",
    "react-datepicker": "^0.61.0",
    "react-dom": "^15.6.2",
    "react-dropzone": "^3.13.4",
    "react-redux": "^4.4.9",
    "react-router": "^3.2.1",
    "react-validation": "^2.10.9",
    "reactable": "^0.14.1",
    "redux": "^3.7.2",
    "redux-immutablejs": "0.0.8",
    "redux-localstorage": "^0.4.0",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^1.7.2",
    "validator": "^6.1.0",
    "webpack": "^1.13.0"
  },
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-plugin-module-resolver": "^2.4.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.26.0",
    "coveralls": "^2.13.3",
    "enzyme": "^2.9.1",
    "eslint": "^3.10.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-react": "^6.7.1",
    "jsdom": "^9.12.0",
    "nyc": "^11.9.0",
    "react-test-renderer": "^15.6.2",
    "redux-ava": "^2.2.0",
    "sinon": "^1.17.3",
    "tap-nyan": "^1.1.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.22.2"
  },
  "ava": {
    "files": [
      "test/**/*.spec.js",
      "test/**/*.spec.jsx"
    ],
    "source": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "concurrency": 10,
    "failFast": true,
    "failWithoutAssertions": false,
    "tap": false,
    "powerAssert": false,
    "require": [
      "babel-register",
      "./test/test-browser-env.js"
    ],
    "babel": "inherit"
  }
}
