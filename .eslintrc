{
  "extends": "airbnb",
  "env": {
    "mocha": true
  },
  "rules": {
    "react/jsx-indent": [2, 2],
    "react/jsx-no-bind": 0,
    "max-len": 1,
    "prefer-arrow-callback": 1,
    "react/no-deprecated": 2,
    "react/prop-types": 1,
    "no-console": 0,
    "indent": [2, 2, {"SwitchCase": 1}],
    "quotes": [2, "double", "avoid-escape"],
    "no-shadow": 1
  }
}
